   "source": [
    "import torch\n",
    "import math\n",
    "\n",
    "delta = 0.5\n",
    "epsilon = 1e-6\n",
    "#acts = torch.zeros(1)\n",
    "acts = torch.Tensor([delta])\n",
    "\n",
    "scale = (2.0 / delta) * math.log((1.0 - epsilon) / epsilon)\n",
    "shifted = acts - (delta / 2.0)\n",
    "penalty = torch.sigmoid(scale * shifted)\n",
    "\n",
    "print(f\"{penalty=}\")"
   ]
  }
